{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "func-call-spacing": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            4
        ],
        "max-len": [
            "error",
            120
        ],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1,
                "maxEOF": 1
            }
        ],
        "no-trailing-spaces": "error",
        "comma-dangle": [
            "error",
            {
                "objects": "never",
                "arrays": "never"
            }
        ],
        "padded-blocks": "off"
    }
}
